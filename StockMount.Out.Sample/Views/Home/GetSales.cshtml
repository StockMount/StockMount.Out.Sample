        <br /> https://out.stockmount.com/api/Integration/GetSales
    </p>

    <a class="btn btn-success" id="btn-test">@Message.GetMessage(MessageCode.TestService, Culture) </a>

    <div class="well display-none margin-top-10" id="test-area">

        <div class="form-group">
            <label for="store">@Message.GetMessage(MessageCode.StoreId, Culture) <span class="label label-danger">@Message.GetMessage(MessageCode.Required, Culture)</span></label>
            <input type="text" class="form-control" id="store" placeholder="@Message.GetMessage(MessageCode.StoreId, Culture)" value="1">
        </div>
        <div class="form-group">
            <label for="orderStatus">@Message.GetMessage(MessageCode.OrderStatus, Culture) <span class="label label-danger">@Message.GetMessage(MessageCode.Required, Culture)</span></label>
            <input type="text" class="form-control" id="orderStatus" placeholder="@Message.GetMessage(MessageCode.OrderStatus, Culture)" value="1">
        </div>
        <div class="form-group">
            <label for="smallestOrderDetailId">@Message.GetMessage(MessageCode.SmallestOrderDetailId, Culture)  <br /><small>@Message.GetMessage(MessageCode.SmallestOrderDetailIdDescription, Culture)</small> </label>
            <input type="text" class="form-control" id="smallestOrderDetailId" placeholder="@Message.GetMessage(MessageCode.SmallestOrderDetailId, Culture)" value="">
        </div>
        <div class="form-group">
            <label for="biggestOrderDetailId">@Message.GetMessage(MessageCode.BiggestOrderDetailId, Culture) <br /><small>@Message.GetMessage(MessageCode.BiggestOrderDetailIdDescription, Culture)</small> </label>
